{
  "defaultSeverity": "error",
  "extends": [
    "tslint-react",
    "tslint-sonarts",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "variable-name": false,
    "function-name": false,
    "no-duplicate-imports": false,
    "import-name": false,
    "jsx-boolean-value": false
  }
}
